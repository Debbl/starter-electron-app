{
  "name": "starter-electron-app",
  "version": "1.0.0",
  "packageManager": "pnpm@9.12.3",
  "description": "starter-electron-app",
  "author": "Brendan Dash <me@aiwan.run> (https://aiwan.run/)",
  "license": "MIT",
  "keywords": [],
  "main": "apps/main/dist/index.js",
  "scripts": {
    "dev": "turbo build --filter @starter-electron-app/main && concurrently \"electron .\" \"turbo dev\"",
    "build": "turbo build",
    "build:main": "turbo build:main",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@debbl/eslint-config": "^3.8.3",
    "@types/node": "^22.8.7",
    "concurrently": "^9.0.1",
    "electron": "^33.0.2",
    "eslint": "^9.14.0",
    "lint-staged": "^15.2.10",
    "simple-git-hooks": "^2.11.1",
    "turbo": "^2.2.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
